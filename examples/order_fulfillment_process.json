{
  "nodes": {
    "order_received": { "label": "Order Received", "shape": "oval", "fillcolor": "lightblue", "style": "filled" },
    "inventory_check": { "label": "Check Inventory", "shape": "rectangle", "fillcolor": "lightgreen", "style": "filled" },
    "item_available": { "label": "Item Available?", "shape": "diamond", "fillcolor": "yellow", "style": "filled" },
    "item_allocation": { "label": "Allocate Item", "shape": "rectangle", "fillcolor": "lightcoral", "style": "filled" },
    "shipping": { "label": "Shipping Process", "shape": "rectangle", "fillcolor": "lightpink", "style": "filled" },
    "notify_customer": { "label": "Notify Customer", "shape": "rectangle", "fillcolor": "lightyellow", "style": "filled" },
    "item_not_available": { "label": "Notify Out of Stock", "shape": "rectangle", "fillcolor": "gray", "style": "filled" },
    "done": { "label": "Process Complete", "shape": "oval", "fillcolor": "lightgray", "style": "filled" }
  },
  "connectors": [
    { "from": "order_received", "to": "inventory_check", "label": "Start Process" },
    { "from": "inventory_check", "to": "item_available", "label": "Check Complete" },
    { "from": "item_available", "to": "item_allocation", "label": "Yes", "color": "green" },
    { "from": "item_available", "to": "item_not_available", "label": "No", "color": "red" },
    { "from": "item_allocation", "to": "shipping", "label": "Allocate Complete" },
    { "from": "shipping", "to": "notify_customer", "label": "Shipped" },
    { "from": "notify_customer", "to": "done", "label": "Customer Notified" },
    { "from": "item_not_available", "to": "done", "label": "Out of Stock Notification Sent" }
  ]
}

